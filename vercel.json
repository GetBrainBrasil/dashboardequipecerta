{
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/dados/(.*).xlsx",
      "headers": [
        { "key": "Content-Type", "value": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" },
        { "key": "Cache-Control", "value": "public, max-age=60" }
      ]
    }
  ]
}
